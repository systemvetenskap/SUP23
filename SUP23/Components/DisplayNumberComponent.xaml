<UserControl x:Class="SUP23.Components.DisplayNumberComponent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SUP23.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <StackPanel>
        <TextBlock Text="{Binding Number, FallbackValue='3'}" FontSize="20" />
            <Button Content="Öka" Width="100" Height="30" Command="{Binding IncreaseNumberCommand}" />

        </StackPanel>
    </Grid>
</UserControl>
